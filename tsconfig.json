{
	"compilerOptions": {
		"module": "NodeNext",
		"baseUrl": "./src",
		"sourceMap": true,
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": true,
		"skipLibCheck": true
	},
	"include": ["./src/**/*"],
  "exclude": ["node_modules", "dist"]
}
